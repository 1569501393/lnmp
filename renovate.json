{
  "assignees": ["khs1994"],
  "reviewers": ["khs1994"],
  "baseBranches": ["EOL"],
  "commitBody": "[skip ci]",
  "labels": ["renovateapp"],
  "timezone": "Asia/Shanghai",
  "extends": [
    "config:base",
    "docker:enableMajor"
  ],
  "ignorePaths": [
    "wsl/docker-compose.yml",
    "wsl/Dockerfile",
    "dockerfile/php-fpm/",
    "app/"
  ],
  "docker": {
    "enabled": true
  },
  "docker-compose": {
    "enabled": true,
    "digest": {
      "enabled": false
    }
  },
  "fileMatch": ["docker-compose.bump.yml"]
}
